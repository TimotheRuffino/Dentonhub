<section class="py-7">
  <div class="container">
    <div class="row">
      <div class="col-lg-5 me-auto">
        <%= image_tag("newsletter.svg", style: "width:70%; position: relative; margin-left: 40px;") %>
      </div>
      <div class="col-lg-6 d-flex justify-content-center flex-column ms-auto">
      <h4 class="title">Restez informés !</h4>
        <p class="description">
          Abonnez-vous à notre newsletter et suivez l'avancement du projet chaque semaine !
        </p>
        <form method="" action="">
          <div class="row">
            <div class="col-sm-8">
              <div class="input-group mb-4">
                <span class="input-group-text"><i class="fas fa-envelope"></i></span>
                <input class="form-control" placeholder="Votre Email..." type="text">
              </div>
            </div>
            <div class="col-sm-4">
              <button type="button" class="btn bg-gradient-primary w-100">S'abonner</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
  </div>
</section>

<script type="text/javascript">
    if (document.getElementById('typed')) {
      var typed = new Typed("#typed", {
        stringsElement: '#typed-strings',
        typeSpeed: 40,
        backSpeed: 40,
        backDelay: 300,
        startDelay: 500,
        loop: true
      });
    }
  </script>
